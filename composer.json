{
    "name": "php-flasher/flasher-notyf-laravel",
    "description": "PHP Flasher Laravel adapter for Notyf",
    "type": "library",
    "keywords": [
        "yoeunes",
        "notify",
        "php",
        "laravel",
        "symfony",
        "Lumen",
        "notifications",
        "messages",
        "alerts",
        "pnotify",
        "notyf ",
        "bundle",
        "flex",
        "toastr"
    ],
    "homepage": "https://php-flasher.github.io/",
    "authors": [
        {
            "name": "Younes Khoubza",
            "email": "younes.khoubza@gmail.com",
            "homepage": "https://github.com/yoeunes",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.3",
        "php-flasher/flasher-laravel": "^0.6.1",
        "php-flasher/flasher-notyf": "^0.6.1"
    },
    "autoload": {
        "psr-4": {
            "Flasher\\Notyf\\Laravel\\": ""
        },
        "files": [
            "Resources/functions/notyf.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flasher\\Notyf\\Laravel\\FlasherNotyfServiceProvider"
            ],
            "aliases": {
                "Notyf": "Flasher\\Notyf\\Laravel\\Facade\\Notyf"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
